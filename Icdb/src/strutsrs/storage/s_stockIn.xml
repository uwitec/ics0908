<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="s_stockIn" extends="basedefalut"
		namespace="/stockIn">
		<action name="searchStockIn"
			class="com.action.storage.StockInAction"
			method="searchStockIn">
			<result name="success">/stockIn/stockIn.jsp</result>
		</action>
		<action name="searchStockInOver"
			class="com.action.storage.StockInAction"
			method="searchStockInOver">
			<result name="success">/stockIn/stockInOver.jsp</result>
		</action>
		<action name="searchCheckStockIn"
			class="com.action.storage.StockInAction"
			method="searchCheckStockIn">
			<result name="success">/stockIn/checkStockIn.jsp</result>
		</action>
		<action name="searchApprovalStockIn"
			class="com.action.storage.StockInAction"
			method="searchApprovalStockIn">
			<result name="success">/stockIn/approvalStockIn.jsp</result>
		</action>
		<action name="goAddStockIn"
			class="com.action.storage.StockInAction"
			method="goAddStockIn">
			<result name="success">/stockIn/addStockInManage.jsp</result>
		</action>
		<action name="goEditStockIn"
			class="com.action.storage.StockInAction"
			method="goEditStockIn">
			<result name="success">/stockIn/addStockInManage.jsp</result>
		</action>
		<action name="goCheckStockIn"
			class="com.action.storage.StockInAction"
			method="goCheckStockIn">
			<result name="success">/stockIn/checkStockInManage.jsp</result>
		</action>
		<action name="goApprovalStockIn"
			class="com.action.storage.StockInAction"
			method="goApprovalStockIn">
			<result name="success">/stockIn/approvalStockInManage.jsp</result>
		</action>
		<action name="goStockInOver"
			class="com.action.storage.StockInAction"
			method="goStockInOver">
			<result name="success">/stockIn/stockInOverManage.jsp</result>
		</action>
		<action name="saveStockIn"
			class="com.action.storage.StockInAction"
			method="saveStockIn">
			<result name="success" type="redirect-action">
				searchStockIn
			</result>
		</action>
		<action name="checkStockIn"
			class="com.action.storage.StockInAction"
			method="checkStockIn">
			<result name="success" type="redirect-action">
				searchStockIn
			</result>
		</action>
		<action name="approvalStockIn"
			class="com.action.storage.StockInAction"
			method="approvalStockIn">
			<result name="success" type="redirect-action">
				searchStockIn
			</result>
		</action>
		<action name="showMateriel"
			class="com.action.storage.StockInAction"
			method="addMaterielList">
			<result name="success">
				/stockIn/showMateriel.jsp
			</result>
		</action>
		<action name="showCargoSpace"
			class="com.action.storage.StockInAction"
			method="addCargoSpaceList">
			<result name="success">
				/stockIn/showCargoSpace.jsp
			</result>
		</action>
	</package>
</struts>