<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="stock" extends="basedefalut" namespace="/storage">
<action name="ShowStockList" class="com.action.storage.StockAction" method="getStoreHouseList">
	<result name="success">/storage/stock/StockList.jsp</result>
</action>
<action name="GetStockMaterielList" class="com.action.storage.StockAction"  method="getStockMaterielList">
	<result name="success">/storage/stock/StockMaterielList.jsp</result>
</action>
<action name="AddStock" class="com.action.storage.StockAction" method="addStock">
	<result name="success" type="redirect">ShowStockList.action</result>
</action>
<action name="ShowMaterStore" class="com.action.storage.StockAction" method="showMaterStore">
	<result name="success">/storage/AddTOMateriel.jsp</result>
</action>
</package>
</struts>