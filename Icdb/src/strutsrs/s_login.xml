<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="login" extends="struts-default" namespace="/login">
        <action name="Login" class="com.action.login.LoginAction" method="login">
            <result name="SUCCESS">/main.jsp</result>
            <result name="NULLRESULT">/login.jsp</result>
        </action>
        <action name="Unlogin" class="com.action.login.LoginAction" method="unlogin">
        	<result name="Unload">/login.jsp</result>
        </action>
        <action name="ChangPass" class="com.action.login.UpdatePassWord" method="changePassWord">
        	<result name="success" type="redirect">Unlogin.action</result>
        </action>
    </package>
</struts>