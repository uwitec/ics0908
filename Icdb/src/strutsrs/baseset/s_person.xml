<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="person" extends="struts-default" namespace="/baseset">
<action name="ShowPerson" class="com.action.baseset.PersonAction" method="showPerson">
	<result name="success">/baseset/person/PersonShow.jsp</result>
</action>
<action name="GoAddPerson" class="com.action.baseset.PersonAction" method="goAddPerson">
	<result name="success">/baseset/person/PersonAdd.jsp</result>
</action>
<action name="AddPerson" class="com.action.baseset.PersonAction" method="addPerson">
	<result name="success">/baseset/AddEnd.jsp</result>
</action>
<action name="GetOnePerson" class="com.action.baseset.PersonAction" method="getOnePerson">
	<result name="success">/baseset/person/PersonUpdate.jsp</result>
</action>
<action name="UpdatePerson" class="com.action.baseset.PersonAction" method="updatePerson">
	<result name="success" type="redirect">ShowPerson.action</result>
</action>
<action name="FindPerson" class="com.action.baseset.PersonAction" method="findPerson">
	<result name="success">/baseset/FindPerson.jsp</result>
</action>
</package>
</struts>