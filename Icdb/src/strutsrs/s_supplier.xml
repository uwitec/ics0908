<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="supplier" extends="struts-default" namespace="/baseset">
<action name="ShowSupplier" class="com.action.baseset.SupplierAction" method="showSupplier">
	<result name="success">/baseset/supplier/SupplierShow.jsp</result>
</action>
<action name="GoAddSupplier" class="com.action.baseset.SupplierAction" method="goAddSupplier">
	<result name="success">/baseset/supplier/SupplierAdd.jsp</result>
</action>
<action name="AddSupplier" class="com.action.baseset.SupplierAction" method="addSupplier">
	<result name="success">/baseset/AddEnd.jsp</result>
</action>
<action name="GetOneSupplier" class="com.action.baseset.SupplierAction" method="getOneSupplier">
	<result name="success">/baseset/supplier/SupplierUpdate.jsp</result>
</action>
<action name="UpdateSupplier" class="com.action.baseset.SupplierAction" method="updateSupplier">
	<result name="success" type="redirect">ShowSupplier.action</result>
</action>
</package>
</struts>