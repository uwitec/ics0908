<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="permission" extends="struts-default" namespace="/permission">
        <action name="ShowEmployeeList" class="com.action.permission.PermissionAction" method="showEmployeeList">
            <result name="SUCCESS">/permission/index.jsp</result>
            <result name="NULLRESULT">/index.jsp</result>
        </action>
        <action name="ShowJobList" class="com.action.permission.PermissionAction" method="showJobList">
            <result name="SUCCESS">/permission/index.jsp</result>
            <result name="NULLRESULT">/index.jsp</result>
        </action>
        <action name="ShowPermissioinList" class="com.action.permission.PermissionAction" method="showPermissioinList">
            <result name="SUCCESS">/permission/permissionlist.jsp</result>
            <result name="NULLRESULT">/index.jsp</result>
        </action>
        <action name="UpdatePermission" class="com.action.permission.PermissionAction" method="updatePermission">
            <result name="SUCCESS">/permission/index.jsp</result>
            <result name="NULLRESULT">/index.jsp</result>
        </action>
        <action name="GetTopMenu" class="com.action.permission.PermissionAction" method="getTopMenu">
            <result name="SUCCESS">/pagecom/topmenu.jsp</result>
            <result name="NULLRESULT">/index.jsp</result>
        </action>
    </package>

</struts>