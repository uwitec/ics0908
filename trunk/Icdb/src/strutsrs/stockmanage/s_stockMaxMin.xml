<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package
        name="stockMaxMin"
        extends="basedefalut"
        namespace="/stockmanage">
        <action
            name="ShowMax"
            class="com.action.stockmanage.StockMaxMinAction"
            method="showMax">
            <result
                name="success">/stockmanage/stockMaxMin/StockMax.jsp</result>
        </action>
        <action
            name="ShowMin"
            class="com.action.stockmanage.StockMaxMinAction"
            method="showMin">
            <result
                name="success">/stockmanage/stockMaxMin/StockMin.jsp</result>
        </action>
    </package>
</struts>