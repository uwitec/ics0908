<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="checkStock" extends="struts-default" namespace="/storage">
<action name="ShowCheck" class="com.action.storage.CheckStockAction" method="showCheckStock">
	<result name="success">/storage/checkStock/CheckStockShow.jsp</result>
</action>
<action name="GoAddCheckStock" class="com.action.storage.CheckStockAction" method="goAddCheckStock">
	<result name="success" type="redirect">EditCheckStock.action</result>
</action>
<action name="AddCheckStock" class="com.action.storage.CheckStockAction" method="addCheckStock">
	<result name="success" type="redirect">ShowCheck.action</result>
</action>
<action name="ShowHistroy" class="com.action.storage.CheckStockAction" method="showHistroy">
	<result name="success">/storage/checkStock/CheckHistroy.jsp</result>
</action>
<action name="FindCheck" class="com.action.storage.CheckStockAction" method="findCheck">
	<result name="success">/storage/checkStock/CheckStockOne.jsp</result>
</action>
<action name="UpdateCheckStock" class="com.action.storage.CheckStockAction" method="updateCheckStock">
	<result name="success" type="redirect">ShowCheck.action</result>
</action>
<action name="EditCheckStock" class="com.action.storage.CheckStockAction" method="editCheckStock">
	<result name="success">/storage/checkStock/CheckStockAdd.jsp</result>
</action>
<action name="ShowCheckApprove" class="com.action.storage.CheckStockAction" method="showCheckApprove">
	<result name="success">/storage/checkStock/CheckApprove.jsp</result>
</action>
<action name="GoCheckApprove" class="com.action.storage.CheckStockAction" method="goCheckApprove">
	<result name="success">/storage/checkStock/CheckApproveEdit.jsp</result>
</action>
<action name="UpdateCheckApprove" class="com.action.storage.CheckStockAction" method="updateCheckApprove">
	<result name="success" type="redirect">ShowCheckApprove.action</result>
</action>
</package>
</struts>