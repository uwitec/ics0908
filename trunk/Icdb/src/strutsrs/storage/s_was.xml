<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="was" extends="basedefalut" namespace="/storage">
<action name="ShowWas" class="com.action.storage.WasAction" method="showWasTage">
	<result name="success">/storage/was/WasShow.jsp</result>
</action>
<action name="AddWasTage" class="com.action.storage.WasAction" method="addWasTage">
	<result name="success" type="redirect">ShowWas.action</result>
</action>
<action name="ShowWasHistroy" class="com.action.storage.WasAction" method="showWasHistroy">
	<result name="success">/storage/was/WasHistroy.jsp</result>
</action>
<action name="GetOneWas" class="com.action.storage.WasAction" method="getOneWas">
	<result name="success">/storage/was/WasOne.jsp</result>
</action>
<action name="UpdateOneWas" class="com.action.storage.WasAction" method="updateOneWas">
	<result name="success">/storage/was/WasUpdate.jsp</result>
</action>
<action name="UpdateWasTage" class="com.action.storage.WasAction" method="updateWasTage">
	<result name="success" type="redirect">ShowWas.action</result>
</action>
<action name="GoAddWas" class="com.action.storage.WasAction" method="goAddWas">
	<result name="success">/storage/was/WasAdd.jsp</result>
</action>
<action name="DeleteWas" class="com.action.storage.WasAction" method="deleteWas">
	<result name="success" type="redirect">ShowWas.action</result>
</action>
</package>
</struts>