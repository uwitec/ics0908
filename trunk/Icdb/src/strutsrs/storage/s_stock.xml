<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="stock" extends="struts-default" namespace="/storage">
<action name="ShowStockList" class="com.action.storage.StockStartAction" method="getStoreHouseList">
	<result name="success">/storage/stock/StockList.jsp</result>
</action>
<action name="GetStockMaerielList" class="com.action.storage.StockStartAction"  method="getStockMaerielList">
	<result name="success">/storage/stock/StockMaerielList.jsp</result>
</action>
<action name="AddStock" class="com.action.storage.StockStartAction" method="addStock">
	<result name="success" type="redirect">GetStockMaerielList.action</result>
</action>
</package>
</struts>