<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="employee" extends="struts-default" namespace="/baseset">
<action name="ShowEmployee" class="com.action.baseset.EmployeeAction" method="showEmployee">
	<result name="success">/baseset/employee/EmployeeShow.jsp</result>
</action>
<action name="GoAddEmployee" class="com.action.baseset.EmployeeAction" method="goAddEmployee">
	<result name="success">/baseset/employee/EmployeeAdd.jsp</result>
</action>
<action name="AddEmployee" class="com.action.baseset.EmployeeAction" method="addEmployee">
	<result name="success">/baseset/AddEnd.jsp</result>
</action>
<action name="UpdateEmployee" class="com.action.baseset.EmployeeAction" 

method="updateEmployee">
	<result name="success" type="redirect">ShowEmployee.action</result>
</action>
<action name="GetOneEmployee" class="com.action.baseset.EmployeeAction" 

method="getOneEmployee">
	<result name="success">/baseset/employee/EmployeeUpdate.jsp</result>
</action>
<action name="FindEmployee" class="com.action.baseset.EmployeeAction" method="findEmployee">
	<result name="success">/baseset/FindEmployee.jsp</result>
</action>
<action name="SelectEmployeeDef" class="com.action.baseset.EmployeeAction" method="selectEmployeeDef">
	<result name="success">/baseset/employee/EmployeeShow.jsp</result>
</action>
</package>
</struts>