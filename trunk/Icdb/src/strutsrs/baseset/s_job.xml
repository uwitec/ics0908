<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="job" extends="struts-default" namespace="/baseset">
<action name="ShowJob" class="com.action.baseset.JobAction" method="showJob">
	<result name="success">/baseset/job/JobShow.jsp</result>
</action>
<action name="GoAddJob" class="com.action.baseset.JobAction" method="goAddJob">
	<result name="success">/baseset/job/JobAdd.jsp</result>
</action>
<action name="AddJob" class="com.action.baseset.JobAction" method="addJob">
	<result name="success" type="redirect">GoAddJob.action</result>
</action>
<action name="GetOneJob" class="com.action.baseset.JobAction" method="getOneJob">
	<result name="success">/baseset/job/JobUpdate.jsp</result>
</action>
<action name="UpdateJob" class="com.action.baseset.JobAction" method="updateJob">
	<result name="success" type="redirect">ShowJob.action</result>
</action>
<action name="FindJob" class="com.action.baseset.JobAction" method="findJob">
	<result name="success">/baseset/FindJob.jsp</result>
</action>
<action name="SelectJobDef" class="com.action.baseset.JobAction" method="selectJobDef">
	<result name="success">/baseset/job/JobShow.jsp</result>
</action>
</package>
</struts>