<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
<package name="cargoSpace" extends="struts-default" namespace="/baseset">
<action name="ShowCargoSpace" class="com.action.baseset.CargoSpaceAction" method="showCargoSpace">
	<result name="success">/baseset/cargoSpace/CargoSpaceShow.jsp</result>
</action>
<action name="AddCargoSpace" class="com.action.baseset.CargoSpaceAction" method="addCargoSpace">
	<result name="success">/baseset/cargoSpace/CargoSpaceShow.jsp</result>
</action>
<action name="UpdateCargoSpace" class="com.action.baseset.CargoSpaceAction" method="updateCargoSpace">
	<result name="success">/baseset/cargoSpace/CargoSpaceShow.jsp</result>
</action>
</package>
</struts>